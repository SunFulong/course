doctype html
form(action= '/search.course', method= 'post')
  input(name='course')
  input(type='submit')
if list
  ul
    li
      a(href='../')= '../'
    each item in list
      li
        unless item.type
          a(href='#{item.name}/')= item.name + '/'
        else
          a(href='#{item.name}')= item.name
else
  = url
